{
  "name": "next-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "npx prettier --write --ignore-unknown 'src/**/*'",
    "generate": "graphql-codegen  --require dotenv/config --config codegen.ts dotenv_config_path=./.env.local && npm run format",
    "seed": "npx ts-node scripts/seed.ts",
    "test:watch": "jest --watch",
    "test": "jest --forceExit",
    "test:ci": "jest --ci",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-query-devtools": "^4.26.1",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "dotenv": "^16.0.3",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "graphql-request": "5.1.0",
    "next": "13.2.3",
    "pg": "^8.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/pg": "^8.6.6",
    "autoprefixer": "^10.4.13",
    "graphql": "^16.6.0",
    "graphql-codegen-apollo-next-ssr": "^1.7.4",
    "hasura-cli": "^2.20.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "ngrok": "^4.3.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npm run format"
    ]
  }
}
