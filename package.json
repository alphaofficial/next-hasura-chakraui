{
  "name": "next-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "sh ./build.sh",
    "start": "next",
    "start:dev": "bash ./start.sh",
    "dev": "next dev",
    "lint": "next lint",
    "format": "npx prettier --write --ignore-unknown 'src/**/*'",
    "generate": "graphql-codegen  --require dotenv/config --config codegen.ts",
    "seed": "npx ts-node --project tsconfig.seed.json scripts/seed.ts",
    "test:watch": "jest --watch",
    "test": "jest --forceExit",
    "test:ci": "jest --ci",
    "prepare": "husky install",
    "console": "npx hasura console --no-browser",
    "metadata:export": "npx hasura metadata export",
    "metadata:apply": "npx hasura metadata apply",
    "metadata:reload": "npx hasura metadata reload",
    "metadata:status": "npx hasura metadata inconsistency status",
    "metadata:diff": "npx hasura metadata inconsistency list",
    "migration:initial": "npx hasura migrate create 'init' --from-server --database-name default",
    "migration:status": "npx hasura migrate status  --database-name default",
    "migration:create": "npx hasura migrate create",
    "migration:apply": "npx hasura migrate apply --database-name default"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@clerk/nextjs": "^4.29.3",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/mulish": "^4.5.14",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-query-devtools": "^4.26.1",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "framer-motion": "^10.2.4",
    "graphql-request": "5.1.0",
    "jest-mock-extended": "^3.0.4",
    "js-cookie": "^3.0.1",
    "next": "13.2.3",
    "next-connect": "^0.13.0",
    "pg": "^8.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/js-cookie": "^3.0.3",
    "@types/pg": "^8.6.6",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "graphql": "^16.6.0",
    "graphql-codegen-apollo-next-ssr": "^1.7.4",
    "hasura-cli": "^2.20.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "ngrok": "^4.3.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npm run format"
    ]
  }
}
